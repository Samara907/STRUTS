<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC ".//Apache Software Foundation/DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.multipart.maxSize" value="104857600"></constant>
	<package name="board" extends="struts-default">
		<interceptors>
			<interceptor name="ibatis" class="struts.board.interceptor.IbatisInterceptor"></interceptor>
		</interceptors>
	
		<action name="listAction" class="struts.board.action.ListAction">
			<result>/board/boardList.jsp</result>
		</action>
		
	</package>
</struts>
